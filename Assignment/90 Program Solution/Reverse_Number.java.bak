import java.util.*;
public class Reverse_Number 
{
	public static void main(String[] args) 
	{
	
		Scanner scn=new Scanner(System.in);
		
		System.out.println("Enter Number ");
		int no=scn.nextInt();
		int rev=0;
		while(no>0){
			no=no%10;
			rev=rev*10+no;
			no/10;
		}

		System.out.println("Reverse Number "+rev);
	}
}
