package com.dheerendra.priority;

public class Pattern {
	public static void main(String[] args) {
		int i,j,k;
		  for(i=1;i<=5;i++)
		  {
		    for(j=1;j<=5;j++)
		    {
		      if(j<=i)
		     System.out.println(j);
		      else
		        System.out.println(" ");
		    }
		    for(j=5;j>=1;j--)
		    {
		      if(j<=i)
		    System.out.println(j);
		      else
		    System.out.println(" ");
		    } 
		    System.out.println();
		  }
		  
		}
}
