package com.dheerendra.Synchronized;

public class Add2 {

	static int x;
	static int y;
	static void add(int x,int y) {
		x=x;
		y=y;
		try {
			Thread.sleep(1000);
		}catch (Exception e) {
			// TODO: handle exception
		e.printStackTrace();
		}
		int res=x+y;
		System.out.println(" in "+Thread.currentThread().getName()+" result"+res);
		
	}

	
}
