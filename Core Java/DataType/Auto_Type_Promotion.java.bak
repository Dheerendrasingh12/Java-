
class Auto_Type_Promotion 
{
	public static void main(String[] args) 
	{
		int i1=12;
		float f1=14;
//		int i=(int)i1+f1;
		int i=i1+(int)f1;
		int i2=(int)(i1+f1);
		System.out.println(i+"  "+i2);
		byte b1=60;
		byte b2=100;
		byte b3=(byte)(b1+b2);
		System.out.println(b3);

		byte b6=20;
		byte b4=40;
		//byte b5=b6+b4;
		byte b7=20+40;
		System.out.println(b7);

		System.out.println(4*2+8/2-3*3+6/3);

		//int d=12/0;
		float f=0.0f/0.0f;
		System.out.println(f);

	
	}
}
